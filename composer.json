{
    "name": "PytoMVC app",
    "type": "project",
    "description": "",
    "license": "MIT",
    "require": {
        "php": ">=5.6.4",
        "pytocryto/pytomvc": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "App\\" : "app/"
        },
        "files": ["app/Helpers/helpers.php"]
